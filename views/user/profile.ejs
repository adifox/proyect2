        <div class="col-sm-12 pics">
            <i class="fa fa-folder-open"></i> Upload Pictures
            <form class="pic-form" action="/profile/upload" method="post" enctype="multipart/form-data">
                <input type="text" name="name" placeholder="picture name">
                <input class="choose-file" type="file" name="file">
                <input class="pic-input-button" type="submit" value="Save">
            </form>
            <section class="user-pics">

              <% if (pictures === []) { %>
                <img src="" alt="No Picture" />
              <% }else{ %>
                <% pictures.forEach(function(picture) { %>
                  <img src="<%= picture.pic_path %>" alt="<%= picture.pic_name %>" />
                <% })%>
              <% }%>

            </section>
        </div>
    </div>
